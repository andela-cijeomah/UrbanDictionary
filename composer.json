{
  "name": "dictionary",
  "description": "a dictionary of city slangs",
  "keywords": [
    "urban",
    "dictionary"
  ],
  "require": {
    "php" : ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Dictionary\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dictionary\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
